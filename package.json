	"scripts": {
		"dev": "run-p dev:*",
		"dev:next": "next dev",
		"dev:db": "docker-compose up -d",
		"dev:ws": "env-cmd ts-node-dev --project tsconfig.server.json --respawn --transpile-only src/server/wssDevServer.ts",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"postinstall": "prisma generate",
